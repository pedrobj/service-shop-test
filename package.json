{
  "name": "service-shop-test",
  "version": "1.0.0",
  "description": "test + hot reload boilerplate + custom configs",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint src",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.production.js",
    "test": "cross-env NODE_ENV=test ava --verbose"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.26.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-ava": "^2.4.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "jsdom": "^11.2.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-ava": "^2.2.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cross-env": "^3.1.4",
    "flexboxgrid": "^6.3.1",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "revalidator": "^0.3.1",
    "string-mask": "^0.3.0",
    "sweetalert2": "^6.9.1"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "require": [
      "./src/util/test-server-setup.js",
      "babel-register"
    ],
    "babel": "inherit"
  }
}
